    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Thorn_Whip -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Thorn_Whip">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Cantrip"/>
        <DnD_basetruth:HasSpellSchool rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Transmutation"/>
        <DnD_basetruth:HasCastingTime>1 action</DnD_basetruth:HasCastingTime>
        <DnD_basetruth:HasDescription>You create a long, vine-like whip covered in thorns that lashes out at your command toward a creature in range. Make a melee spell attack against the target. If the attack hits, the creature takes 1d6 piercing damage, and if the creature is Large or smaller, you pull the creature up to 10 feet closer to you.

This spell’s damage increases by 1d6 when you reach 5th level (2d6), 11th level (3d6), and 17th level (4d6).</DnD_basetruth:HasDescription>
        <DnD_basetruth:HasSpellComponents>V, S, M (the stem of a plant with thorns)</DnD_basetruth:HasSpellComponents>
        <DnD_basetruth:HasSpellDuration>Instantaneous</DnD_basetruth:HasSpellDuration>
        <DnD_basetruth:HasSpellRange>30 feet</DnD_basetruth:HasSpellRange>

    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Power_Word_Heal -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Power_Word_Heal">

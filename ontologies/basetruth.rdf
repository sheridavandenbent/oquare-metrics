        <DnD_basetruth:HasDescription>The ancient traditions of worship and sacrifice must be preserved and upheld. (Lawful)</DnD_basetruth:HasDescription>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Tradition_Songs -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Tradition_Songs">

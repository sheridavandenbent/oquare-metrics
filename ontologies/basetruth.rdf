    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Incendiary_Cloud -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Incendiary_Cloud">

    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Divine_Smite -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Divine_Smite">

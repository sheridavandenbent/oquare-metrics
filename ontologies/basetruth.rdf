        <DnD_basetruth:HasRacialIncreaseFrom rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Elf"/>
        <DnD_basetruth:HasRacialIncreaseFrom rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Forest_Gnome"/>
        <DnD_basetruth:HasRacialIncreaseFrom rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Halfling"/>
        <DnD_basetruth:HasRacialIncreaseFrom rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Human"/>
        <DnD_basetruth:IsImportantAbilityScoreFor rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Monk"/>
        <DnD_basetruth:IsImportantAbilityScoreFor rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Ranger"/>
        <DnD_basetruth:IsImportantAbilityScoreFor rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Rogue"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Diamond_Soul -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Diamond_Soul">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Class_Feature"/>
        <DnD_basetruth:IsClassFeatureOf rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Monk"/>

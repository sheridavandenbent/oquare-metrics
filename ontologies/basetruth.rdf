    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Maul -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Maul">

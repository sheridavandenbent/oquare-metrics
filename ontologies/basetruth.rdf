    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Spare_the_Dying -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Spare_the_Dying">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Cantrip"/>
        <DnD_basetruth:HasSpellSchool rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Necromancy"/>

    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Halfling_Nimbleness -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Halfling_Nimbleness">

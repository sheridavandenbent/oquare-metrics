    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Common_Clothes -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Common_Clothes">

    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Divine_Sense -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Divine_Sense">

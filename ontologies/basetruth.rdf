    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Staggering_Smite -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Staggering_Smite">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#4th_Level_Spell"/>
        <DnD_basetruth:HasSpellSchool rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Evocation"/>
        <DnD_basetruth:HasCastingTime>1 bonus action</DnD_basetruth:HasCastingTime>
        <DnD_basetruth:HasDescription>The next time you hit a creature with a melee weapon attack during this spell’s duration, your weapon pierces both body and mind, and the attack deals an extra 4d6 psychic damage to the target. The target must make a Wisdom saving throw. On a failed save, it has disadvantage on attack rolls and ability checks, and can’t take reactions, until the end of its next turn.</DnD_basetruth:HasDescription>
        <DnD_basetruth:HasSpellComponents>V</DnD_basetruth:HasSpellComponents>
        <DnD_basetruth:HasSpellDuration>Concentration, up to 1 minute</DnD_basetruth:HasSpellDuration>
        <DnD_basetruth:HasSpellRange>Self</DnD_basetruth:HasSpellRange>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Steal_Valuables -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Steal_Valuables">

    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Pass_Without_Trace -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Pass_Without_Trace">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#2nd_Level_Spell"/>
        <DnD_basetruth:HasSpellSchool rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Abjuration"/>
        <DnD_basetruth:HasCastingTime>1 action</DnD_basetruth:HasCastingTime>
        <DnD_basetruth:HasDescription>A veil of shadows and silence radiates from you, masking you and your companions from detection. For the duration, each creature you choose within 30 feet of you (including you) has a +10 bonus to Dexterity (Stealth) checks and can’t be tracked except by magical means. A creature that receives this bonus leaves behind no tracks or other traces of its passage.</DnD_basetruth:HasDescription>
        <DnD_basetruth:HasSpellComponents>V, S, M (ashes from a burned leaf of mistletoe and a sprig of spruce)</DnD_basetruth:HasSpellComponents>
        <DnD_basetruth:HasSpellDuration>Concentration, up to 1 hour</DnD_basetruth:HasSpellDuration>
        <DnD_basetruth:HasSpellRange>Self</DnD_basetruth:HasSpellRange>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Passwall -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Passwall">

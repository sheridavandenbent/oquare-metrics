    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Tongues -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Tongues">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#3rd_Level_Spell"/>
        <DnD_basetruth:HasSpellSchool rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Divination"/>
        <DnD_basetruth:HasCastingTime>1 action</DnD_basetruth:HasCastingTime>
        <DnD_basetruth:HasDescription>This spell grants the creature you touch the ability to understand any spoken language it hears. Moreover, when the target speaks, any creature that knows at least one language and can hear the target understands what it says.</DnD_basetruth:HasDescription>
        <DnD_basetruth:HasSpellComponents>V, M (a small clay model of a ziggurat)</DnD_basetruth:HasSpellComponents>
        <DnD_basetruth:HasSpellDuration>1 hour</DnD_basetruth:HasSpellDuration>
        <DnD_basetruth:HasSpellRange>Touch</DnD_basetruth:HasSpellRange>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Too_Greedy -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Too_Greedy">

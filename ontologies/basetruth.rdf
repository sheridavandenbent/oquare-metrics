    <!-- http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Revenge_Dead_Lover -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Revenge_Dead_Lover">
        <rdf:type rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Bond"/>
        <DnD_basetruth:IsBondOf rdf:resource="http://www.semanticweb.org/sherida/ontologies/2023/6/DnD_basetruth#Charlatan"/>
